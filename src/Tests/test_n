int
main()
{
    int[5] i;
    bool[5] j;
    int k;

    k = 0;
    print("Enter 5 ints\n");
    while(k < 5)
    {
        read(i[k]);
        k = k + 1;
    }
    print("Entered:", i[0], i[1], i[2], i[3], i[4], "\n");

    k = 0;
    print("Enter 5 bools\n");
    while(k < 5)
    {
        read(j[k]);
        k = k + 1;
    }
    print("Entered:", j[0], j[1], j[2], j[3], j[4], "\n");

    print("4th Int:", i[1 + 2], "\n");
    print("1st Int:", i[3 * 2 - 6], "\n");
}
